package  {	import flash.display.MovieClip;	import flash.events.Event;		public class competidores extends MovieClip {		public var xInicial:Number = 50;		public var yInicial:Number = 50;		public var velocidade:Number = 2;		public var i:Number=0				public function competidores() {			this.x = xInicial;			this.y = yInicial;			addEventListener(Event.ENTER_FRAME, move_competidor);		}				private function move_competidor(evt:Event):void {										if (this.hitTestObject(MovieClip(parent).trackPoints[i])){				i++;				}			if (MovieClip(parent).trackPoints.length == i){				i=0;				}						var distX:Number = MovieClip(parent).trackPoints[i].x - this.x;			var distY:Number = MovieClip(parent).trackPoints[i].y - this.y;			var angulo_rad:Number = Math.atan2(distY,distX);			var xVel=Math.cos(angulo_rad)*velocidade;			var yVel=Math.sin(angulo_rad)*velocidade;			this.y +=yVel;			this.x +=xVel;		}	}	}