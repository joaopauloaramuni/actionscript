package  {		import flash.display.MovieClip;	import flash.events.*;	import flash.net.*;		public class main extends MovieClip{				private var xmlURL:URLRequest = new URLRequest("questoes.xml");		private var xmlArmazem:URLLoader = new URLLoader(xmlURL);		private var conteudoXML:XML				public function main() {			xmlArmazem.addEventListener(Event.COMPLETE, xmlCarregado);		}						private function xmlCarregado(event:Event) {			conteudoXML  = XML(event.target.data);			trace("Enunciados: " + conteudoXML.questao.enunciado);			trace("Enunciado Q1: " + conteudoXML.questao[1].enunciado);			trace("2a Resposta para a Q2: " + conteudoXML.questao[1].respostas.resposta[1]);			trace("Tipo da 2a Resposta para a Q2: " + conteudoXML.questao[1].respostas.resposta[1].@tipo);		}	}	}