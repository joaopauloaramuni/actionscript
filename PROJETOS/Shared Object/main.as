package  {		import flash.display.MovieClip;	import flash.events.*;	import flash.net.*;		public class main extends MovieClip{		private var jogadoresInfo:Array = new Array();		private var jogadorInfo:Object = new Object();		private var informacoesLocais:SharedObject = SharedObject.getLocal("teste");				public function main() {			btCriaDados.addEventListener(MouseEvent.CLICK,criaDados);			btGravaDados.addEventListener(MouseEvent.CLICK,gravaDados);			btRecuperaDados.addEventListener(MouseEvent.CLICK,recuperaDados);		}				private function criaDados(evt:MouseEvent):void {						for (var i:int=0; i<10 ; i++){				var jogadorInfo:Object = new Object();				jogadorInfo.nome = "Jogador"+i;				jogadorInfo.score = int(Math.random()*10);				jogadoresInfo.push(jogadorInfo);				trace(jogadorInfo.nome,jogadorInfo.score);			}					}				public function gravaDados(evt:MouseEvent):void {			informacoesLocais.data.gameinfo = jogadoresInfo;			informacoesLocais.flush();		}				public function recuperaDados(evt:MouseEvent):void {			jogadoresInfo = informacoesLocais.data.gameinfo;			trace(jogadoresInfo[3].nome,jogadoresInfo[3].score);		}	}	}