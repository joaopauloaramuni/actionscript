package  {				import flash.display.MovieClip;	import flash.events.*;	import flash.net.*;	import flash.system.fscommand;			public class recebeDados extends MovieClip{				private var teste:Number =2		//cria instância da classe LocalConnection		var conexaoLocal:LocalConnection = new LocalConnection();		public function recebeDados() {			//DEFINE O NOME DA CONEXÃO			conexaoLocal.connect("conexao_1");			//DEFINE O SWF COMO CLIENTE - RECEBERÁ O CONTEÚDO			conexaoLocal.client = this;			//OBS.: o nome da variável que contém a instância da Classe "LocalConnection()"			//não precisa ser o mesmo do swf que envia o conteúdo.			//O que deve ser o mesmo é o nome atribuído a propriedade connect (neste caso conexao_1)		}				public function exibeConteudo(recebeConteudo:String):String {			//função executada pelo outro swf			caixaDeTexto.text = recebeConteudo;			return "teste"		}	}	}