package  {		import flash.display.MovieClip;	import flash.events.*;		public class Bala extends MovieClip{		var anguloBala:Number		var tamanhoVetor:Number		var velocidadeBala:Number = 20				public function Bala(angulo) {			//tipo de bala como argumento			anguloBala = angulo-90			this.addEventListener(Event.ENTER_FRAME,moveBala);		}		public function moveBala(event:Event) {//não precisa ser pública				var radianos:Number=anguloBala*(Math.PI/180);				var xVel=Math.cos(radianos)*velocidadeBala;				var yVel=Math.sin(radianos)*velocidadeBala;				this.y +=yVel;				this.x +=xVel;		}	}}